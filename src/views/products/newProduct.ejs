    <h1>New product</h1>
    <form action="/products" method="POST">
      <div>
        <label for="">Nombre: </label>
        <input type="text" name="nombre">
      </div>
      <div>
        <label for="">Precio: </label>
        <input type="text" name="precio">
      </div>
      <div>
        <label for="">Descripcion: </label>
